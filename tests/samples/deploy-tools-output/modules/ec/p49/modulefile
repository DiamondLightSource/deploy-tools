#%Module1.0
##
## ec - ec command line tool configured for p49
##
module-whatis "ec command line tool configured for p49"

if { [module-info mode load] } {
    if { [is-loaded ec] } {
      module unload ec
    }
}

setenv EC_TARGET "p49-beamline/p49"
setenv EC_K8S_NAMESPACE "p49-beamline"
setenv EC_SERVICES_REPO "https://github.com/epics-containers/p49-services.git"

module load edge-containers-cli
module load pollux/local

prepend-path PATH "/tmp/deploy-tools-output/modules/ec/p49/entrypoints"
