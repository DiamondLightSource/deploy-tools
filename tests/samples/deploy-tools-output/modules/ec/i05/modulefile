#%Module1.0
##
## ec - ec command line tool configured for i05
##
module-whatis "ec command line tool configured for i05"

if { [module-info mode load] } {
    if { [is-loaded ec] } {
      module unload ec
    }
}

setenv EC_TARGET "i05-beamline/i05"
setenv EC_SERVICES_REPO "https://gitlab.diamond.ac.uk/controls/containers/beamline/i05-services.git"

module load edge-containers-cli
module load k8s-i05/local

prepend-path PATH "/tmp/deploy-tools-output/modules/ec/i05/entrypoints"
