#%Module1.0
##
## ec - ec command line tool configured for p38
##
module-whatis "ec command line tool configured for p38"

if { [module-info mode load] } {
    if { [is-loaded ec] } {
      module unload ec
    }
}

setenv EC_TARGET "p38-beamline/p38"
setenv EC_SERVICES_REPO "https://gitlab.diamond.ac.uk/controls/containers/beamline/p38-services.git"

module load edge-containers-cli
module load k8s-p38/local

prepend-path PATH "/tmp/deploy-tools-output/modules/ec/p38/entrypoints"
