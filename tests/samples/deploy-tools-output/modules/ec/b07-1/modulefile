#%Module1.0
##
## ec - ec command line tool configured for b07-1
##
module-whatis "ec command line tool configured for b07-1"

if { [module-info mode load] } {
    if { [is-loaded ec] } {
      module unload ec
    }
}

setenv EC_TARGET "b07-1-beamline/b07-1"
setenv EC_K8S_NAMESPACE "b07-1-beamline"
setenv EC_SERVICES_REPO "https://gitlab.diamond.ac.uk/controls/containers/beamline/b07-1-services.git"

module load edge-containers-cli
module load k8s-b07-1/0.1

prepend-path PATH "/tmp/deploy-tools-output/modules/ec/b07-1/entrypoints"
